//This programme take number of array and gives you sum of array elements

public class Main
{
	public static void main(String[] args) {
	    java.util.Scanner kb=new java.util.Scanner(System.in);
	    System.out.print("Please enter number of array: ");
	    int x=kb.nextInt();
	    int array[]=new int [x];
	    
	    for (int number=0; number<x; number++)
	    {
	        System.out.println("Please enter number of "+(number+1));
	        array[number]=kb.nextInt();

	    }
	    System.out.println("Elements of array: ");
	    
	    for (int i=0; i<x; i++){
	        System.out.println(array[i]);
	    } 
	    int sum=0;
	    
	    for(int i=0; i<x; i++)
	        sum+=array[i];
	        System.out.print("Sum of array: "+ sum);
	}
}
